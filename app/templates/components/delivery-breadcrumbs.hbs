{{breadcrumb-item label="Home"
                  routeName="index"
                  selectedRouteName=selectedRouteName}}
{{#if deliveryIndex}}
  {{breadcrumb-item label="Deliveries"
                    routeName="deliveries.index"
                    selectedRouteName=selectedRouteName}}
  {{#if deliveryShow}}
    {{breadcrumb-item label=transfer.project.name
                      routeName="deliveries.show"
                      context=delivery
                      selectedRouteName=selectedRouteName}}
    {{#if deliveryShowResend }}
      {{breadcrumb-item label="resend"
                        routeName="deliveries.show.resend"
                        context=transfer
                        selectedRouteName=selectedRouteName}}
    {{/if}}
  {{else if deliveryNew}}
    {{breadcrumb-item label="New"
                      routeName="deliveries.new"
                      selectedRouteName=selectedRouteName}}
  {{/if}}
{{else if dukeDsProjectIndex}}
  {{breadcrumb-item label="Duke DS Projects"
                    routeName="duke-ds-projects.index"
                    selectedRouteName=selectedRouteName}}
  {{#if dukeDsProjectShow}}
    {{breadcrumb-item label=project.name
                      routeName="duke-ds-projects.show"
                      context=project
                      selectedRouteName=selectedRouteName}}

  {{/if}}
{{/if}}
{{yield}}
